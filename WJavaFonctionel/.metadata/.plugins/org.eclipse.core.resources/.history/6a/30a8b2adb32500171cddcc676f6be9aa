
package ch.arc.cours.a_intro.comparator;

import java.util.Comparator;

public class ComparatorSeparer implements Comparator<Double>
	{
	
	/*------------------------------------------------------------------*\
	|*							Constructeurs							*|
	\*------------------------------------------------------------------*/
	
	/*------------------------------------------------------------------*\
	|*							Methodes Public							*|
	\*------------------------------------------------------------------*/
	
	/**
	 * 1 	si v1 >v2
	 * 0 	si v1=v2
	 * -1 	si v1<v2
	 */
	@Override public int compare(Double v1, Double v2)
		{
		return v1.compareTo(v2);
		}
		
	/*------------------------------------------------------------------*\
	|*							Methodes Private						*|
	\*------------------------------------------------------------------*/
	
	/*------------------------------------------------------------------*\
	|*							Attributs Private						*|
	\*------------------------------------------------------------------*/
	}
