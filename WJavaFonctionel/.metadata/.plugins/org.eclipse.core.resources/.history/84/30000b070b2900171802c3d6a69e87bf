
package ch.arc.cours.lamda.interfacefonctionelle.neu.custom;



public class FunctionShop
	{
	
	
	
	/*------------------------------------------------------------------*\
	|*							Methodes Public							*|
	\*------------------------------------------------------------------*/
	
	public static double carrer(double x)
		{
		return x*x;
		}
		
	public static double add1(double x)
		{
		return x+1;
		}
	
	/*------------------------------------------------------------------*\
	|*							Methodes Private						*|
	\*------------------------------------------------------------------*/
	
	
	}

