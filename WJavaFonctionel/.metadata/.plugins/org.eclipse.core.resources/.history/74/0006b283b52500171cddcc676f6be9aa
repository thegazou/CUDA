
package ch.arc.cours.a_intro.comparator.tools;

import java.util.Arrays;
import java.util.Random;

public class ComparatorTools
	{

	/*------------------------------------------------------------------*\
	|*							Methodes Public							*|
	\*------------------------------------------------------------------*/

	public static Banane[] createTab(int n)
		{
		Random random = new Random();

		Banane[] tab = new Banane[n];

		for(int i = 1; i <= n; i++)
			{
			tab[i] = new Banane(random.nextInt(),random.nextDouble());
			}

		return tab;
		}

	public static void print(Banane[] tab)
		{
		System.out.println(Arrays.toString(tab));
		}

	/*------------------------------------------------------------------*\
	|*							Methodes Private						*|
	\*------------------------------------------------------------------*/

	}
