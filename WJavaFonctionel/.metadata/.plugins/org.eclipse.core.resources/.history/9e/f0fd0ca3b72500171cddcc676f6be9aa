
package ch.arc.cours.a_intro.actionlistener;

import javax.swing.JFrame;

public class UseJFrameHello
	{

	/*------------------------------------------------------------------*\
	|*							Methodes Public							*|
	\*------------------------------------------------------------------*/

	public static void main(String[] args)
		{
		main();
		}

	public static void main()
		{
		UseJFrameHello u=new UseJFrameHello();
		u.setVisible(true);
		System.out.println("ici");
		JFrame f=new JFrame();
		f.setVisible(true);

		}

	/*------------------------------------------------------------------*\
	|*							Methodes Private						*|
	\*------------------------------------------------------------------*/

	}

