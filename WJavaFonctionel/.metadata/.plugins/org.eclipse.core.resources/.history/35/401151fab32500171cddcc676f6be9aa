
package ch.arc.cours.a_intro.comparator;

import java.util.Random;

public class ComparatorTools
	{
	
	/*------------------------------------------------------------------*\
	|*							Methodes Public							*|
	\*------------------------------------------------------------------*/
	
	public double[] createTab(int n)
		{
		Random random = new Random();
		
		double[] tab = new double[n];
		
		for(int i = 1; i <= n; i++)
			{
			tab[i] = random.nextDouble();
			}
			
		return tab;
		}
		
	/*------------------------------------------------------------------*\
	|*							Methodes Private						*|
	\*------------------------------------------------------------------*/
	
	}
