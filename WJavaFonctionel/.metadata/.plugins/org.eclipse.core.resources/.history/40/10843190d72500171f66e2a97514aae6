
package ch.arc.cours.lamda.stream.tools;

public class Personne
	{

	/*------------------------------------------------------------------*\
	|*							Constructeurs							*|
	\*------------------------------------------------------------------*/

	public Personne(int age, int hauteur)
		{
		this.age = age;
		this.hauteur = hauteur;
		}

	/*------------------------------------------------------------------*\
	|*							Methodes Public							*|
	\*------------------------------------------------------------------*/

	

	/*------------------------------*\
	|*				Get				*|
	\*------------------------------*/

	
	public int getHauteur()
		{
		return this.hauteur;
		}

	/*------------------------------*\
	|*				Set				*|
	\*------------------------------*/

	

	public void setHauteur(int hauteur)
		{
		this.hauteur = hauteur;
		}

	/*------------------------------------------------------------------*\
	|*							Methodes Private						*|
	\*------------------------------------------------------------------*/

	/*------------------------------------------------------------------*\
	|*							Attributs Private						*|
	\*------------------------------------------------------------------*/

	// Inputs/Outputs
	private int age;
	private int hauteur;

	}
