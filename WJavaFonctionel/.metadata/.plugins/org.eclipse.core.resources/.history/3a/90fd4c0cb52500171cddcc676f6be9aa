
package ch.arc.cours.a_intro.comparator;

import java.util.Comparator;

public class ComparatorSeparer implements Comparator<Banane>
	{

	/*------------------------------------------------------------------*\
	|*							Constructeurs							*|
	\*------------------------------------------------------------------*/

	/*------------------------------------------------------------------*\
	|*							Methodes Public							*|
	\*------------------------------------------------------------------*/

	/**
	 * 1 	si v1 >v2
	 * 0 	si v1=v2
	 * -1 	si v1<v2
	 */
	@Override public int compare(Banane b1, Banane b2)
		{
		return v1.compareTo(v2);
		}

	/*------------------------------------------------------------------*\
	|*							Methodes Private						*|
	\*------------------------------------------------------------------*/

	/*------------------------------------------------------------------*\
	|*							Attributs Private						*|
	\*------------------------------------------------------------------*/
	}
