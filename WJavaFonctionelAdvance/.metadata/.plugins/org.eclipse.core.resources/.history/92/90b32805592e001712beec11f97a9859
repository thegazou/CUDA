
package ch.arc.cours.lamda.interfacefonctionelle.b_neu.d_function.math.original;

import org.junit.Assert;

public class UseBiFunctionMath
	{

	/*------------------------------------------------------------------*\
	|*							Methodes Public							*|
	\*------------------------------------------------------------------*/

	public static void main(String[] args)
		{
		main();
		}

	public static void main()
		{
		System.out.println("math : BiFunction");

		biFunction();
		}

	/*------------------------------------------------------------------*\
	|*							Methodes Private						*|
	\*------------------------------------------------------------------*/

	private static void biFunction()
		{
		add1();
		add2();
		}

	/*------------------------------*\
	|*		BiFunction<T1,T2,T3>	*|
	\*------------------------------*/

	/**
	 * f(x1,x2)= (x1-x2) + (x1*x2)  ;
	 *
	 * h(x1,x2)= x1 - x2
	 * g(x1,x2)= x1 * x2
	 * f(x1,x2) = (g + h)(x1,x2) = h(x1,x2) + g(x1,x2)
	 */
	private static void add1()
		{
		//lamda dans variable
			{
			// TODO
			BiFunctionDouble f =null;

			Assert.assertTrue(f.apply(3d, 2d) == 7);
			}
		}

	/**
	 * f(x1,x2)= (x1-x2) + (x1*x2)  ;
	 *
	 * h(x1,x2)= x1 - x2
	 * g(x1,x2)= x1 * x2
	 * f(x1,x2) = (g + h)(x1,x2) = h(x1,x2) + g(x1,x2)
	 */
	private static void add2()
		{
		//lamda dans variable
			{
			// TODO
			BiFunctionDouble h =null;
			BiFunctionDouble g =null;
			BiFunctionDouble f =null;

			Assert.assertTrue(f.apply(3d, 2d) == 7);
			}
		}

	}
